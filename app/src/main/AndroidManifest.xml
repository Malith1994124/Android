<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.malith.test2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Appointment Management System"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:icon="@drawable/ic_launcher"
        android:label="@string/main_appointmentHeader" >
        <activity
            android:name=".App.systemactivity"
            android:label="Appointment Management System" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".App.createAppintmnet">
            android:label="@string/main_appointmentHeader" >
            <intent-filter>
                <action android:name="android.intent.action.App.createAppintmnet" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".App.delbydate"></activity>
        <activity android:name=".App.ViewEditAppointments"></activity>
        <activity android:name=".App.MoveAppointment"></activity>
        <activity android:name=".App.SearchAppointments"></activity>
    </application>



</manifest>
